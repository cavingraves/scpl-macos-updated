{
	"name": "scpl-macos-updated",
	"version": "2.3.1",
	"description": "Code your Apple Shortcuts in text! 495 actions for macOS automation. AI-powered creation with Claude. Text-based shortcut programming language. Vibe code your workflows instead of clicking. Supports Apple Intelligence, Voice Memos, Clock automation, Window management, and more.",
	"main": "built/index.js",
	"types": "built/index.d.ts",
	"license": "MIT",
	"homepage": "https://github.com/cavingraves/scpl-macos-updated",
	"bugs": {
		"url": "https://github.com/cavingraves/scpl-macos-updated/issues"
	},
	"keywords": [
		"shortcuts",
		"apple-shortcuts",
		"macos-automation",
		"shortcut-programming",
		"text-based-shortcuts",
		"code-shortcuts",
		"vibe-coding",
		"ai-automation",
		"claude-integration",
		"natural-language-programming",
		"apple-intelligence",
		"voice-memo-automation",
		"clock-automation",
		"window-manager",
		"accessibility-automation",
		"homekit-scripting",
		"ios",
		"macos",
		"tahoe",
		"automation",
		"scpl",
		"shortcut-language",
		"apple-silicon",
		"intel",
		"workflow-automation",
		"productivity-automation",
		"scripting-language"
	],
	"scripts": {
		"docs": "tsc && node ./built/src/GenDocs",
		"test": "jest --no-cache --coverage",
		"actions": "tsc && node ./built/src/Data/GenActions",
		"format": "yarn run prettier --write src/*** test/*.ts test/*.json index.ts .prettierrc .eslintrc.js modules.d.ts plistconv.js .travis.yml",
		"prepublishOnly": "yarn actions && yarn format && yarn docs && yarn test"
	},
	"author": {
		"name": "pfg",
		"email": "omegariisthebest@gmail.com",
		"url": "https://pfgithub.github.io/"
	},
	"contributors": [
		{
			"name": "Cavin Graves",
			"email": "193660148+cavingraves@users.noreply.github.com",
			"url": "https://github.com/cavingraves"
		}
	],
	"repository": "github:cavingraves/scpl-macos-updated",
	"files": [
		"/man",
		"/built/index.js",
		"/built/index.d.ts",
		"/built/src"
	],
	"jest": {
		"transform": {
			"^.+\\.tsx?$": "ts-jest"
		},
		"moduleFileExtensions": [
			"ts",
			"tsx",
			"js",
			"jsx",
			"json"
		],
		"testMatch": [
			"**/test/**/*.[jt]s?(x)",
			"**/?(*.)+(spec|test).[tj]s?(x)"
		],
		"testPathIgnorePatterns": [
			"/node_modules/",
			"/built"
		],
		"coverageReporters": [
			"json",
			"lcov",
			"clover"
		]
	},
	"dependencies": {
		"bplist-creator": "^0.0.7",
		"bplist-parser": "^0.1.1",
		"plist": "^3.0.1",
		"uuid": "^3.3.2"
	},
	"devDependencies": {
		"@types/jest": "^24.0.15",
		"@types/node": "^11.9.5",
		"@types/uuid": "^3.4.4",
		"@types/yargs": "^12.0.9",
		"@typescript-eslint/eslint-plugin": "^1.4.2",
		"@typescript-eslint/parser": "^1.4.2",
		"ava-ts": "^0.25.2",
		"codecov": "^3.5.0",
		"eslint": "^5.14.1",
		"jest": "^24.8.0",
		"prettier": "^1.16.4",
		"ts-jest": "^24.0.2",
		"ts-node": "^8.0.3",
		"typescript": "^3.3.3333",
		"webpack-cli": "^3.2.3"
	}
}
